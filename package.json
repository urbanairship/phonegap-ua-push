    "cordova-android",
    "cordova-ios"
  ],
  "engines": [
    {
      "name": "cordova-android",
      "version": ">=4.1.0"
    },
    {
      "name": "cordova-ios",
      "version": ">=4.3.0"
    },
    {
      "name": "cordova-plugman",
      "version": ">=4.2.0"
    }
  ],
  "author": "Urban Airship",
  "license": "Apache 2.0",
  "homepage": "https://github.com/urbanairship/urbanairship-cordova.git",
  "devDependencies": {
    "adm-zip": "^0.4.13",
    "co": "^4.6.0",
    "co-prompt": "^1.0.0",
    "commander": "^2.19.0",
    "fs": "0.0.1-security",
    "jsdoc": "3.5.5",
    "minami": "1.2.3",
    "semver": "^5.6.0",
    "superagent": "^4.1.0"
  },
  "scripts": {
    "generate-docs": "jsdoc ./www/UrbanAirship.js -t ./node_modules/minami/ -r jsdoc_readme.md",
