{
  "name": "urbanairship-cordova",
  "version": "7.6.0",
  "description": "Urban Airship Cordova plugin",
  "cordova": {
    "id": "urbanairship-cordova",
    "platforms": [
      "android",
      "ios"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/urbanairship/urbanairship-cordova.git"
  },
  "keywords": [
    "cordova",
    "urbanairship",
